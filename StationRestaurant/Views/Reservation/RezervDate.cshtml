@model  ReservationPostVM
<main>
    <section class="ttt">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="header-title mb-4">Basic Form</h4>
                                        <form class="parsley-examples" method="post" enctype="multipart/form-data">
                                            <div class="form-group">
                                                <label asp-for="Name">Reservation Date<span class="text-danger">*</span></label>
                                                <input asp-for="Name" parsley-trigger="change" required placeholder="Enter Reservation Date" class="form-control">
                                                <span asp-validation-for="Name" class="text-danger"></span>
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="LastName">Reservation Date<span class="text-danger">*</span></label>
                                                <input asp-for="LastName" parsley-trigger="change" required placeholder="Enter Reservation Date" class="form-control">
                                                <span asp-validation-for="LastName" class="text-danger"></span>
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="PhoneNumber">Reservation Date<span class="text-danger">*</span></label>
                                                <input asp-for="PhoneNumber" parsley-trigger="change" required placeholder="Enter Reservation Date" class="form-control">
                                                <span asp-validation-for="PhoneNumber" class="text-danger"></span>
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="Email">Reservation Date<span class="text-danger">*</span></label>
                                                <input asp-for="Email" parsley-trigger="change" required placeholder="Enter Reservation Date" class="form-control">
                                                <span asp-validation-for="Email" class="text-danger"></span>
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="Additionals">Reservation Date<span class="text-danger">*</span></label>
                                                <input asp-for="Additionals" parsley-trigger="change" required placeholder="Enter Reservation Date" class="form-control">
                                                <span asp-validation-for="Additionals" class="text-danger"></span>
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="TableID">Reservation Date<span class="text-danger">*</span></label>
                                                <input asp-for="TableID" parsley-trigger="change" required placeholder="Enter Reservation Date" class="form-control">
                                                <span asp-validation-for="TableID" class="text-danger"></span>
                                            </div>
                                            <div class="form-group">
                                                <label asp-for="ReservDate">Reservation Date<span class="text-danger">*</span></label>
                                                <input asp-for="ReservDate" parsley-trigger="change" required placeholder="Enter Reservation Date" class="form-control">
                                                <span asp-validation-for="ReservDate" class="text-danger"></span>
                                            </div>
                                            <div class="form-group text-right mb-0">
                                                <button class="btn btn-primary waves-effect waves-light mr-1" type="submit">
                                                    Submit
                                                </button>
                                                <button type="reset" class="btn btn-secondary waves-effect waves-light">
                                                    Cancel
                                                </button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <!-- end card -->
                            </div>
                            <!-- end col -->

                        </div>

                </div>
            </div>
        </div>
    </section>


</main>